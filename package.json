	"jest": {
		"verbose": true,
		"globals": {
			"__DEV__": true
		},
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"json",
			"jsx",
			"node"
		],
		"moduleNameMapper": {
			"@/pages/OAuthConfigurationService": "<rootDir>/src/pages/OAuthConfigurationService.tsx",
			"@/pages/pic/SSOSignInButton/Helpers/noHashQueryStringUtils": "<rootDir>/src/pages/pic/SSOSignInButton/Helpers/noHashQueryStringUtils.js",
