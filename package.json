		"@types/uuid": "^8.3.0",
		"@types/yup": "^0.29.9",
		"@typescript-eslint/eslint-plugin": "^5.10.2",
		"@typescript-eslint/parser": "^5.10.2",
		"cross-env": "^7.0.2",
		"cypress": "^6.4.0",
		"cypress-cucumber-preprocessor": "^4.0.0",
		"eslint": "^8.8.0",
		"eslint-config-react-app": "^7.0.0",
		"eslint-webpack-plugin": "^2.2.0",
		"prettier": "^2.1.2",
		"puppeteer": "^7.1.0",
		"semver": "^7.3.2",
		"ts-jest": "^26.4.4",
		"typescript": "^4.0.5"
